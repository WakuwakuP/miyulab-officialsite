{
  "name": "with-firebase-hosting-and-typescript",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C && cpx \"dist/functions/next/service-worker.js\" \"dist/public\" -C ",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tslint --project src/functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@firebase/app": "^0.7.14",
    "@firebase/firestore": "^3.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "dotenv": "^14.3.0",
    "dotenv-webpack": "^7.0.3",
    "firebase": "^9.6.4",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.16.0",
    "next": "^12.0.8",
    "next-offline": "^5.0.5",
    "next-page-transitions": "^1.0.0-canary.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^7.0.5",
    "react": "^17.0.2",
    "react-display-switch": "^0.4.3",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-lazyload": "^3.2.0",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.2.6",
    "react-textarea-autosize": "^8.3.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/styled-jsx": "^2.2.9",
    "babel-plugin-redux-saga": "^1.1.2",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "firebase-tools": "^10.1.2",
    "rimraf": "3.0.2",
    "styled-jsx-plugin-stylelint": "^0.1.0",
    "tsc": "^2.0.4",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^4.5.5"
  }
}